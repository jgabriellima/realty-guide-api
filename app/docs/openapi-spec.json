          "property_id": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ]
          },
          "parameter_name": {
            "type": "string"
          },
          "parameter_value_description": {
            "type": "string"
          }
        },
        "type": "object",
        "required": [
          "parameter_name",
          "parameter_value_description"
        ]
