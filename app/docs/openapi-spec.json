          "whatsapp_number"
        ]
      },
      "PreferencesRequest": {
        "properties": {
          "whatsapp_number": {
            "type": "string"
          },
          "parameter_name": {
            "type": "string"
          },
          "parameter_value_description": {
            "type": "string"
          }
        },
        "type": "object",
        "required": [
          "whatsapp_number",
          "parameter_name",
          "parameter_value_description"
        ]
      },
      "Property": {
        "properties": {
          "id": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ]
          },
          "id_reference": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "slug": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "total_price": {
            "type": "number"
          },
          "iptu": {
            "type": "number"
          },
          "condominium_fee": {
            "type": "number"
          },
          "neighborhood": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          },
          "full_address": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          },
          "property_metadata": {
            "items": {
              "$ref": "#/components/schemas/PropertyMetadata"
            },
            "type": "array",
            "default": []
          },
          "assistant_instructions": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "type": "object",
        "required": [
          "id_reference",
          "title",
          "description",
          "slug",
          "url",
          "total_price",
          "iptu",
          "condominium_fee"
        ]
      },
      "PropertyLookupRequest": {
        "properties": {
          "url": {
            "type": "string"
          },
          "real_estate_agent_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "integer"
              }
            ]
          }
        },
        "type": "object",
        "required": [
          "url",
          "real_estate_agent_id"
        ]
      },
      "PropertyMetadata": {
        "properties": {
          "id": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ]
