        "type": "object",
        "required": [
          "name",
          "whatsapp"
        ]
      },
      "SchedulerReminderRequest": {
        "properties": {
          "real_estate_agent_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "integer"
              }
            ]
          },
          "remainder_description": {
            "type": "object"
          },
          "remainder_time_in_seconds": {
            "type": "integer"
          }
        },
        "type": "object",
        "required": [
          "real_estate_agent_id",
          "remainder_description",
          "remainder_time_in_seconds"
        ]
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array"
          },
          "msg": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ]
