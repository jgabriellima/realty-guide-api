              }
            ]
          },
          "name": {
            "type": "string"
          },
          "whatsapp": {
            "type": "string"
          }
        },
        "type": "object",
        "required": [
          "name",
          "whatsapp"
        ]
      },
      "EnrichPropertyDataRequest": {
        "properties": {
          "property_slug": {
